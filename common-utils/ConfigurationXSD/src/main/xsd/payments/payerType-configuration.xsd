<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.example.org/payerType-configuration" xmlns="http://www.example.org/payerType-configuration" elementFormDefault="qualified">

	<xs:element name="payerTypes">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="payerType" maxOccurs="unbounded">
					<xs:complexType>
						<xs:attribute name="code" type="xs:string" use="required"/>
						<xs:attribute name="value" type="xs:string"  use="required"/>
						<xs:attribute name="active" type="xs:boolean"  use="required"/>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>


</xs:schema>
