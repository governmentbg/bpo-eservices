<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:tm="http://www.tm-xml.org/XMLSchema/common" targetNamespace="http://www.example.org/entitlementsOpponent-configuration"
	xmlns="http://www.example.org/entitlementsOpponent-configuration"
	elementFormDefault="qualified">

	<xs:element name="entitlementsOpponent">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="entitlementOpponent" maxOccurs="unbounded">
					<xs:complexType>
						<xs:simpleContent>
							<xs:extension base="xs:string">
								<xs:attribute name="code" type="xs:string" use="required" />
								<xs:attribute name="applicationType" type="xs:string" use="required" />
								<xs:attribute name="earlierEntitlementRightType" type="xs:string" use="required"/>
								<xs:attribute name="mandatoryDetails" type="xs:boolean" use="required" />
								<xs:attribute name="showDetails" type="xs:boolean" use="required" />
								<xs:attribute name="enabled" type="xs:boolean" use="required" />
							</xs:extension>
						</xs:simpleContent>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>

</xs:schema>
