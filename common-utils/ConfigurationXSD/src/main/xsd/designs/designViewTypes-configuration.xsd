<?xml version="1.0" encoding="UTF-8"?>

<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.example.org/designViewTypes-configuration" xmlns="http://www.example.org/designViewTypes-configuration" elementFormDefault="qualified">

	<xs:element name="designViewTypes">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="designViewType" maxOccurs="unbounded">
					<xs:complexType>
						<xs:attribute name="code" type="xs:string" use="required"/>
						<xs:attribute name="value" type="xs:string" use="required"/>
						<xs:attribute name="order" type="xs:int" use="required" />
						<!-- xs:attribute name="name" type="xs:string"  use="required"/ -->
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>

</xs:schema>
