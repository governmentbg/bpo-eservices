<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.example.org/permissions" xmlns="http://www.example.org/permissions" elementFormDefault="qualified">

	<xs:element name="roles">
		<xs:complexType>
			<xs:sequence >
				<xs:element name="role" type="roleType" maxOccurs="unbounded" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:complexType name="roleType">
		<xs:sequence>
			<xs:element name="permissions" type="permissionsType"/>
		</xs:sequence>
		<xs:attribute name="name" type="xs:string" use="required"/>
	</xs:complexType>
	
	<xs:complexType name="permissionsType">
		<xs:sequence>
			<xs:element name="permission" type="tm-efiling-permissions" maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>
	
	
