<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://www.example.org/test/"
        targetNamespace="http://www.example.org/test/">

    <complexType name="sectionRights">
        <sequence>
            <element name="name" type="string" minOccurs="0" maxOccurs="1"/>
            <element name="link" type="string" minOccurs="0" maxOccurs="1"/>
            <element name="rights" type="string" minOccurs="0" maxOccurs="unbounded"/>
        </sequence>
    </complexType>

    <element name="sectionPermissions">
        <complexType>
            <sequence>
                <element name="sectionRights" type="tns:sectionRights" minOccurs="0" maxOccurs="unbounded" />
            </sequence>
        </complexType>
    </element>

</schema>