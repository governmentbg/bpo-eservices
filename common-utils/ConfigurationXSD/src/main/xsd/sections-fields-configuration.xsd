			<element name="fastTrack" type="boolean" maxOccurs="1" minOccurs="0"/> <!-- Default Value when null : false -->
		</sequence>

		<attribute name="id" type="tns:availableSectionId" use="required" />
		<attribute name="description" type="string"/>
	</complexType>

