			<element name="editableImport" type="boolean" maxOccurs="1" minOccurs="0"/> <!-- Default Value when null : true -->
			<element name="usable" type="boolean" maxOccurs="1" minOccurs="0"/>
			<element name="defaultValue" type="string" maxOccurs="1" minOccurs="0"/>
			<element name="fastTrack" type="boolean" maxOccurs="1" minOccurs="0"/> <!-- Default Value when null : false -->
			<element name="fastTrackValues" type="string" minOccurs="0"/>
		</sequence>
		<attribute name="id" type="string" use="required"></attribute>
	</complexType>

