		<sequence>
			<element name="visible" type="boolean" maxOccurs="1" minOccurs="1" />
			<element name="selectable" type="boolean" minOccurs="0" maxOccurs="1" default="true"/> <!-- Default Value when null : true -->
			<element name="required" type="boolean" maxOccurs="1" minOccurs="1" />
			<element name="format" type="string" maxOccurs="1" minOccurs="0" /> <!-- Default Value when null : "" -->
			<element name="previousCTMImportable" type="boolean" maxOccurs="1" minOccurs="0"/><!-- Default Value when null : true -->
			<element name="priorityImportable" type="boolean" maxOccurs="1" minOccurs="0"/><!-- Default Value when null : true -->
