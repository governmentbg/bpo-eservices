	<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<parent>
		<groupId>eu.ohim.sp.common</groupId>
		<artifactId>common</artifactId>
		<version>2.0.0-RELEASE</version>
	</parent>

	<artifactId>ConfigurationXSD</artifactId>
	<packaging>jar</packaging>

    <properties>
        <sonar.skip>true</sonar.skip>
    </properties>

	<dependencies>
        <dependency>
            <groupId>eu.ohim.sp.common</groupId>
            <artifactId>CommonUtils</artifactId>
        </dependency>
	</dependencies>

	<build>
		<resources>
			<resource>
				<directory>${basedir}/src/main/xsd</directory>
				<filtering>true</filtering>
				<includes>
					<include>**/*.*</include>
				</includes>
			</resource>
		</resources>

		<pluginManagement>
			<plugins>
				<!--This plugin's configuration is used to store Eclipse m2e settings 
					only. It has no influence on the Maven build itself. -->
				<plugin>
					<groupId>org.eclipse.m2e</groupId>
					<artifactId>lifecycle-mapping</artifactId>
					<version>1.0.0</version>
					<configuration>
						<lifecycleMappingMetadata>
							<pluginExecutions>
								<pluginExecution>
									<pluginExecutionFilter>
										<groupId>
											org.jvnet.jaxb2.maven2
										</groupId>
										<artifactId>
											maven-jaxb2-plugin
										</artifactId>
										<versionRange>
											[0.8.1,)
										</versionRange>
										<goals>
											<goal>generate</goal>
										</goals>
									</pluginExecutionFilter>
									<action>
										<ignore />
									</action>
								</pluginExecution>
							</pluginExecutions>
						</lifecycleMappingMetadata>
					</configuration>
				</plugin>
			</plugins>
		</pluginManagement>

		<plugins>
			<!-- JAXB -->
			<plugin>
				<groupId>org.jvnet.jaxb2.maven2</groupId>
				<artifactId>maven-jaxb2-plugin</artifactId>
				<version>0.8.1</version>
                <dependencies>
                    <dependency>
                        <groupId>eu.ohim.sp.common</groupId>
                        <artifactId>SPMavenPlugin</artifactId>
                        <version>${pm.sp.common.current.version}</version>
                    </dependency>
                </dependencies>
				<configuration>
					<forceRegenerate>true</forceRegenerate>
					<args>
						<arg>-Xvalue-constructor</arg>
                        <arg>-Xprivate</arg>
					</args>
					<plugins>
						<plugin>
							<groupId>org.jvnet.jaxb2_commons</groupId>
							<artifactId>jaxb2-value-constructor</artifactId>
							<version>3.0</version>
						</plugin>
                        <plugin>
                            <groupId>eu.ohim.sp.common</groupId>
                            <artifactId>SPMavenPlugin</artifactId>
                            <version>${pm.sp.common.current.version}</version>
                     </plugin>
               </plugins>
           </configuration>

           <executions>
              <execution>
                  <id>fileupload-configuration</id>
                  <goals>
                      <goal>generate</goal>
                  </goals>
                  <configuration>
                      <generatePackage>eu.ohim.sp.core.configuration.domain.xsd</generatePackage>
                      <schemaDirectory>src/main/xsd</schemaDirectory>
                      <generateDirectory>src/main/java</generateDirectory>
                      <bindingDirectory>src/main/xsd</bindingDirectory>
                      <bindingIncludes>
                          <bindingInclude>jaxb-bindings.xjb</bindingInclude>
                      </bindingIncludes>
                  </configuration>
              </execution>


              <execution>
                  <id>language-configuration</id>
                  <goals>
