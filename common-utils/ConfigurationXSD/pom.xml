              <execution>
                  <id>design-configuration</id>
                  <goals>
                      <goal>generate</goal>
                  </goals>
                  <configuration>
                      <generatePackage>eu.ohim.sp.core.configuration.domain.design.xsd</generatePackage>
                      <schemaDirectory>src/main/xsd/designs</schemaDirectory>
