			<wsdl:output name="getTaxonomyResponse" message="tns:getTaxonomyResponse" />
			<wsdl:fault name="niceClassificationFault" message="tns:niceClassificationFault" />
		</wsdl:operation>
	</wsdl:portType>

	<wsdl:binding name="NiceClassificationServiceBinding" type="tns:NiceClassificationServicePortType">
		<soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http" />
		
		<wsdl:operation name="verifyListOfTerms">
