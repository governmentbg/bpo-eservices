			<wsdl:output name="getTaxonomyResponse">
				<soap:body use="literal" />
			</wsdl:output>
            <wsdl:fault name="niceClassificationFault">
                <soap:fault use="literal" name="niceClassificationFault"/>
            </wsdl:fault>
        </wsdl:operation>
	</wsdl:binding>

	<wsdl:service name="NiceClassificationService">
		<wsdl:port name="NiceClassificationServicePort" binding="tns:NiceClassificationServiceBinding" >
			<soap:address location="http://${fsp.ws.host}/${fsp.ws.context.root}${fsp.ws.eu.ohim.sp.external.adapters.classification.path}${fsp.ws.wsdl.services.path}" />
		</wsdl:port>
