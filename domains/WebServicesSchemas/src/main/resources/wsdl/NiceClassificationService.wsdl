			<wsdl:output name="verifyListOfTermsResponse">
				<soap:body use="literal" />
			</wsdl:output>
            <wsdl:fault name="niceClassificationFault">
                <soap:fault use="literal" name="niceClassificationFault"/>
            </wsdl:fault>
        </wsdl:operation>
		<wsdl:operation name="searchTerm">
			<soap:operation soapAction="" style="document" />
			<wsdl:input name="searchTerm">
				<soap:body use="literal" />
			</wsdl:input>
			<wsdl:output name="searchTermResponse">
				<soap:body use="literal" />
			</wsdl:output>
            <wsdl:fault name="niceClassificationFault">
                <soap:fault use="literal" name="niceClassificationFault"/>
            </wsdl:fault>
		</wsdl:operation>
		<wsdl:operation name="getDistribution">
			<soap:operation soapAction="" style="document" />
