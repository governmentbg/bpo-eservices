<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
		xsi:noNamespaceSchemaLocation="ehcache.xsd" updateCheck="true" monitoring="autodetect" dynamicConfig="true">

<defaultCache
        maxElementsInMemory="4096"
        eternal="false"
        timeToIdleSeconds="120"
        timeToLiveSeconds="120"
        overflowToDisk="false"
        memoryStoreEvictionPolicy="LRU"/>
<cache name="importedNiceClassHeading"         
		maxElementsInMemory="4096"
        eternal="false"
        timeToIdleSeconds="43200"
        timeToLiveSeconds="43200"
        overflowToDisk="false"
        memoryStoreEvictionPolicy="LRU"/>
<cache name="validatedNiceClassHeading"         
		maxElementsInMemory="4096"
        eternal="false"
        timeToIdleSeconds="43200"
        timeToLiveSeconds="43200"
        overflowToDisk="false"
        memoryStoreEvictionPolicy="LRU"/>
<cache name="sortedSections" 
		maxElementsInMemory="4096"
        eternal="false"
        timeToIdleSeconds="120"
        timeToLiveSeconds="120"
        overflowToDisk="false"
        memoryStoreEvictionPolicy="LRU"/>
<cache name="sortedSubsections"
		maxElementsInMemory="4096"
        eternal="false"
        timeToIdleSeconds="43200"
        timeToLiveSeconds="43200"
        overflowToDisk="false"
        memoryStoreEvictionPolicy="LRU"/>

<cache name="displayNavigationPath" eternal="true" memoryStoreEvictionPolicy="FIFO"/>
<cache name="classScopes" 
		maxElementsInMemory="4096"
        eternal="false"
        timeToIdleSeconds="43200"
        timeToLiveSeconds="43200"
        overflowToDisk="false"
        memoryStoreEvictionPolicy="LRU"/>
        
<cache name="getTaxonomy" 
		maxElementsInMemory="100"
        eternal="false"
        timeToIdleSeconds="43200"
        timeToLiveSeconds="43200"
        overflowToDisk="false"
        memoryStoreEvictionPolicy="LRU"/>
<cache name="searchTerms" 
		maxElementsInMemory="4096"
        eternal="false"
        timeToIdleSeconds="43200"
        timeToLiveSeconds="43200"
        overflowToDisk="false"
        memoryStoreEvictionPolicy="LRU"/>
		
<cache name="serviceEnabled" 
		maxElementsInMemory="200"
        eternal="false"
        timeToIdleSeconds="120"
        timeToLiveSeconds="120"
        overflowToDisk="false"
        memoryStoreEvictionPolicy="LRU"/>

	<cache name="isFastTrackField"
		   maxElementsInMemory="4096"
		   eternal="true"
		   memoryStoreEvictionPolicy="FIFO"/>
	<cache name="fastTrackValues"
		   maxElementsInMemory="4096"
		   eternal="true"
		   memoryStoreEvictionPolicy="FIFO"/>


	<cache name="sortedViewStates" eternal="true" memoryStoreEvictionPolicy="FIFO"/>
<cache name="renderField" eternal="true" memoryStoreEvictionPolicy="FIFO"/>
<cache name="requiredField" eternal="true" memoryStoreEvictionPolicy="FIFO"/>
<cache name="formattedField" eternal="true" memoryStoreEvictionPolicy="FIFO"/>
