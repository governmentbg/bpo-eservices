<cache name="importableFields" eternal="true" memoryStoreEvictionPolicy="FIFO"/>
<cache name="allFields" eternal="true" memoryStoreEvictionPolicy="FIFO"/>
<cache name="editableField" eternal="true" memoryStoreEvictionPolicy="FIFO"/>
<cache name="usableField" eternal="true" memoryStoreEvictionPolicy="FIFO"/>
<cache name="defaultValueField" eternal="true" memoryStoreEvictionPolicy="FIFO"/>
<cache name="firstLanguages" eternal="true" memoryStoreEvictionPolicy="FIFO"/>
<cache name="personDetails" eternal="true" memoryStoreEvictionPolicy="FIFO"/>
<cache name="secondLanguages" eternal="true" memoryStoreEvictionPolicy="FIFO"/>
<cache name="countries" eternal="true" memoryStoreEvictionPolicy="FIFO"/>
<cache name="countriesObject" eternal="true" memoryStoreEvictionPolicy="FIFO"/>
<cache name="nationalities" eternal="true" memoryStoreEvictionPolicy="FIFO"/>
