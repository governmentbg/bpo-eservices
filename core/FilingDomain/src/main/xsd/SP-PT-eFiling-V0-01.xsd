            <xs:enumeration value="Pixel"/>
        </xs:restriction>
    </xs:simpleType>

    <!-- ====================================================================== -->
    <!--PatentDescriptionSheetDetailsType                                       -->
    <!-- ====================================================================== -->
    <xs:complexType name="PatentDescriptionSheetDetailsType">
        <xs:sequence>
            <xs:element name="PatentDescriptionSheet" type="PatentDescriptionSheetType" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <!-- ====================================================================== -->
    <!-- PatentDescriptionSheetDetailsType                                      -->
    <!-- ====================================================================== -->
    <xs:complexType name="PatentDescriptionSheetType">
