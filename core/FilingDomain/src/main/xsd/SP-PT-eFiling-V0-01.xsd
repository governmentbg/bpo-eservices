    <!-- Filename    : ViewKindType.xsd                                         -->
    <!-- Last Update : Mon Oct 24 09:27:21 +0200 2011                           -->
    <!-- ====================================================================== -->
    <xs:simpleType name="ViewKindType">
        <xs:union memberTypes="xs:token">
            <xs:simpleType>
                <xs:restriction base="xs:token">
                    <xs:enumeration value="Image"/>
                    <xs:enumeration value="Drawing"/>
                    <xs:enumeration value="Photograph"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:union>
    </xs:simpleType>

    <!-- ====================================================================== -->
    <!-- Filename    : ViewStatusType.xsd                                       -->
    <!-- Last Update : Mon Oct 24 09:27:21 +0200 2011                           -->
    <!-- ====================================================================== -->
    <xs:simpleType name="ViewStatusType">
        <xs:restriction base="xs:token">
            <xs:enumeration value="Filed"/>
            <xs:enumeration value="Registered"/>
            <xs:enumeration value="Withdrawn"/>
        </xs:restriction>
    </xs:simpleType>

    <!-- ====================================================================== -->
    <!-- Filename    : ViewRepresentationSizeType.xsd                           -->
    <!-- Last Update : Mon Oct 24 09:27:21 +0200 2011                           -->
