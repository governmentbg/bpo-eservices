<?xml version='1.0' encoding='UTF-8'?>
<xs:schema elementFormDefault="unqualified" attributeFormDefault="unqualified"
           version="0.01"
           targetNamespace="http://fsp.ohim.eu/XMLSchema"
           xmlns="http://fsp.ohim.eu/XMLSchema"
           xmlns:xs="http://www.w3.org/2001/XMLSchema">

    <xs:annotation>
        <xs:documentation>
            SP Patent Filing schema created on 2019-04-09,
            based on SP Design Filing schema Version 2.0
            and SP Trademark Filing schema Version 2.0
        </xs:documentation>
    </xs:annotation>

    <!-- ==================================================================== -->
    <!-- External type definitions -->
    <!-- ==================================================================== -->
    <xs:include schemaLocation="external/ISOCountryCodeType-V2011.xsd" />
    <xs:include schemaLocation="external/ISOCountryCodeType-V2011.xsd" />
    <xs:include schemaLocation="external/ISOCurrencyCodeType-V2008.xsd" />
    <xs:include schemaLocation="external/ISOLanguageCodeType-V2002.xsd" />
    <xs:include schemaLocation="external/WIPOST3CodeType-V2011.xsd" />

    <!-- TRANSACTION ELEMENT -->

    <!-- ==================================================================== -->
    <!-- Transaction -->
    <!-- ==================================================================== -->
    <xs:element name="Transaction" type="TransactionType" />

    <!-- TYPES DEFINITION -->

    <!-- ==================================================================== -->
    <!-- TransactionType -->
    <!-- ==================================================================== -->
    <xs:complexType name="TransactionType">
        <xs:sequence>
            <xs:element name="TransactionHeader" type="TransactionHeaderType" minOccurs="0"/>
            <xs:element name="PatentTransactionBody" type="TransactionBodyType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <!-- ====================================================================== -->
    <!-- TransactionHeaderType.xsd -->
    <!-- ====================================================================== -->
    <xs:complexType name="TransactionHeaderType">
        <xs:sequence>
            <xs:element name="SenderDetails" type="SenderDetailsType" minOccurs="0" />
            <xs:element name="ReceiverDetails" type="ReceiverDetailsType" minOccurs="0" />
        </xs:sequence>
    </xs:complexType>

    <!-- ==================================================================== -->
    <!-- TransactionBodyType -->
    <!-- ==================================================================== -->
    <xs:complexType name="TransactionBodyType">
        <xs:sequence>
            <xs:element name="TransactionErrorDetails" type="TransactionErrorDetailsType" minOccurs="0" />
            <xs:element name="TransactionContentDetails" type="TransactionContentDetailsType" />
        </xs:sequence>
    </xs:complexType>

    <!-- ====================================================================== -->
    <!-- Filename : TransactionErrorDetailsType.xsd -->
    <!-- ====================================================================== -->
    <xs:complexType name="TransactionErrorDetailsType">
        <xs:sequence>
            <xs:element name="TransactionError" type="TransactionErrorType" maxOccurs="unbounded" />
        </xs:sequence>
    </xs:complexType>

    <!-- ====================================================================== -->
    <!-- Filename : TransactionErrorType.xsd -->
    <!-- ====================================================================== -->
    <xs:complexType name="TransactionErrorType">
        <xs:sequence>
            <xs:element name="TransactionErrorCode" type="TransactionErrorCodeType" minOccurs="0" />
            <xs:element name="TransactionErrorText" type="TextType" minOccurs="0" />
        </xs:sequence>
    </xs:complexType>

    <!-- ==================================================================== -->
    <!-- TransactionDataType -->
    <!-- ==================================================================== -->
    <xs:complexType name="TransactionDataType">
        <xs:sequence>
            <!-- SP additional elements -->
            <xs:element name="PatentApplication" type="PatentApplicationType" minOccurs="0" />
            <xs:element name="PatentServiceApplication" type="PatentServicesApplicationType"	minOccurs="0" />
            <xs:element name="PaymentDetails" type="PaymentDetailsType" minOccurs="0" />
        </xs:sequence>
    </xs:complexType>

    <!-- ====================================================================== -->
    <!-- Filename    : PatentApplicationType.xsd                                -->
    <!-- Last Update : Mon Oct 24 09:27:21 +0200 2011                           -->
    <!-- ====================================================================== -->
    <xs:complexType name="PatentApplicationType">
        <xs:sequence>
            <xs:element name="ApplicationReferenceNumber" type="xs:string" minOccurs="0" />
            <xs:element name="ApplicationReferenceDate" type="xs:date" minOccurs="0" />

            <xs:element name="DocumentIdentifier" type="xs:token" minOccurs="0"/>
            <xs:element name="RequestSoftware" type="RequestSoftwareType" minOccurs="0"/>
            <xs:element name="ApplicationFormName" type="xs:string" minOccurs="0"/>
            <xs:element name="ApplicationKindName" type="xs:string" minOccurs="0"/>
            <xs:element name="RequestExamination" type="RequestExaminationType" minOccurs="0" />


            <xs:element name="DocumentIncludedDetails" type="DocumentIncludedDetailsType" minOccurs="0" />
            <xs:element name="PatentDetails" type="PatentDetailsType"/>
            <xs:element name="SpcPatentDetails" type="SpcPatentDetailsType" minOccurs="0"/>
            <xs:element name="TechnicalQuestionnaire" type="TechnicalQuestionnaireType"/>
            <xs:element name="CertificateRequestedIndicator" type="xs:boolean" minOccurs="0"/>
            <xs:element name="PaymentDetails" type="PaymentDetailsType" minOccurs="0"/>
            <xs:element name="ReimbursementDetails" type="ReimbursementDetailsType" minOccurs="0"/>
            <xs:element name="SignatoryDetails" type="SignatoryDetailsType" minOccurs="0"/>


            <xs:element name="AnticipationOfPublication" type="xs:boolean" minOccurs="0" />
            <xs:element name="PublicationDefermentIndicator" type="xs:boolean" minOccurs="0"/>
            <xs:element name="PublicationDefermentDocument" type="DocumentType" minOccurs="0" maxOccurs="unbounded" />

            <xs:element name="LicenceAvailabilityIndicator" type="xs:boolean" minOccurs="0"/>
            <xs:element name="RealInventorsDeclarationIndicator" type="xs:boolean" minOccurs="0"/>
            <xs:element name="RealInventorsDocument" type="DocumentType" minOccurs="0" maxOccurs="unbounded" />
            <xs:element name="SmallCompanyDeclarationIndicator" type="xs:boolean" minOccurs="0"/>
            <xs:element name="SmallCompanyDocument" type="DocumentType" minOccurs="0" maxOccurs="unbounded" />
            <xs:element name="EpoDecisionCopyIndicator" type="xs:boolean" minOccurs="0"/>
            <xs:element name="EpoTransferChangeFormIndicator" type="xs:boolean" minOccurs="0"/>
            <xs:element name="RequestExaminationIndicator" type="xs:boolean" minOccurs="0"/>
            <xs:element name="EsignDocDeclarationIndicator" type="xs:boolean" minOccurs="0"/>


            <xs:element name="ClassifiedForNationalSecurityIndicator" type="xs:boolean" minOccurs="0"/>
            <xs:element name="ClassifiedForDefenseIndicator" type="xs:boolean" minOccurs="0"/>

            <!-- Efiling fields -->
            <xs:element name="ApplicationFilingNumber" type="xs:string" minOccurs="0" />
            <xs:element name="ApplicationFilingDate" type="xs:date" minOccurs="0" />
            <xs:element name="ApplicationFilingDateTime" type="xs:dateTime" minOccurs="0" />

            <xs:element name="EntitlementToApply" type="EntitlementToApplyType" minOccurs="0"/>

            <xs:element name="ApplicantsImportedFromTemplate" type="xs:boolean" minOccurs="0"/>

            <xs:element name="Comment" type="xs:string" minOccurs="0" />
        </xs:sequence>
        <xs:attribute name="operationCode" type="OperationCodeType" use="optional"/>
    </xs:complexType>

    <!-- ==================================================================== -->
    <!-- PatentServicesApplicationType -->
    <!-- ==================================================================== -->
    <xs:complexType name="PatentServicesApplicationType">
        <xs:sequence>
            <!-- Standard Application fields -->
            <xs:element name="RequestSoftware" type="RequestSoftwareType" minOccurs="0"/>
            <xs:element name="ApplicationFormName" type="xs:string" minOccurs="0"/>

            <xs:element name="RequestExamination" type="RequestExaminationType" minOccurs="0"/>
            <xs:element name="DocumentIncludedDetails" type="DocumentIncludedDetailsType" minOccurs="0"/>
            <xs:element name="PaymentDetails" type="PaymentDetailsType" minOccurs="0"/>
            <xs:element name="ReimbursementDetails" type="ReimbursementDetailsType" minOccurs="0"/>
            <xs:element name="SignatoryDetails" type="SignatoryDetailsType" minOccurs="0"/>
            <!-- Generic Filing information -->
            <xs:element name="ApplicationFilingNumber" type="xs:string" minOccurs="0" />
            <xs:element name="ApplicationFilingDate" type="xs:date" minOccurs="0" />
            <xs:element name="ApplicationFilingDateTime" type="xs:dateTime" minOccurs="0" />
            <xs:element name="WarningMessages" type="xs:string" minOccurs="0" maxOccurs="unbounded" />
            <xs:element name="Comment" type="xs:string" minOccurs="0" />
            <xs:element name="PagesCountAttachment" type="xs:integer" minOccurs="0" />
            <xs:element name="HolderIsInventor" type="xs:boolean" minOccurs="0" />
            <xs:element name="PartialInvalidity" type="xs:boolean" minOccurs="0" />
            <!-- Generic Application information -->
            <xs:element name="ReceivingOfficeCode" type="WIPOST3CodeType" minOccurs="0" />
            <xs:element name="ReceivingOfficeDate" type="xs:date" minOccurs="0" />
            <xs:element name="ApplicationLanguageCode" type="ExtendedISOLanguageCodeType" minOccurs="0"/>
            <xs:element name="ApplicationSecondLanguageCode" type="ExtendedISOLanguageCodeType" minOccurs="0" />
            <xs:element name="CorrespondenceLanguageCode" type="ExtendedISOLanguageCodeType" minOccurs="0"/>
            <xs:element name="ApplicationNumber" type="xs:string" minOccurs="0" />
            <xs:element name="ApplicationDate" type="xs:date" minOccurs="0" />
            <xs:element name="ExpirationDate" type="xs:date" minOccurs="0" />
            <xs:element name="ApplicationDateTime" type="xs:dateTime" minOccurs="0" />
            <xs:element name="ApplicationReference" type="xs:string" minOccurs="0" />
            <xs:element name="CorrespondenceAddress" type="CorrespondenceAddressType" minOccurs="0" maxOccurs="unbounded" />
            <xs:element name="ApplicationRequestType" type="xs:string" minOccurs="0" />
            <!-- Application persons -->
            <xs:element name="ApplicantDetails" type="ApplicantDetailsType" />
            <xs:element name="RepresentativeDetails" type="RepresentativeDetailsType" minOccurs="0" />
            <xs:element name="HolderDetails" type="HolderDetailsType" minOccurs="0"/>
            <xs:element name="PersonChangeDetails" type="PersonChangeDetailsType" minOccurs="0" maxOccurs="unbounded" />
            <!-- Application Trademarks, Oppositions or Recordals -->
            <xs:element name="PatentDetails" type="PatentDetailsType" minOccurs="0"/>
            <xs:element name="PatentRecordDetails" type="PatentRecordDetailsType" minOccurs="0"/>

            <xs:element name="EsignDocDeclarationIndicator" type="xs:boolean" minOccurs="0"/>
            <xs:element name="ProcessInitiatedBeforePublicationIndicator" type="xs:boolean" minOccurs="0"/>

            <xs:element name="SmallCompany" type="xs:boolean" minOccurs="0" />
            <xs:element name="SmallCompanyFiles" type="DocumentType" minOccurs="0" maxOccurs="unbounded" />
            <xs:element name="LicenceAvailability" type="xs:boolean" minOccurs="0" />

            <xs:element name="OppositionDetails" type="OppositionDetailsType" minOccurs="0"/>
            <xs:element name="MarketPermissionDetails" type="MarketPermissionDetailsType" minOccurs="0"/>

            <xs:element name="SelectedUserdoc" type="UserdocType" minOccurs="0"/>
            <xs:element name="RelateRequestToObject" type="xs:boolean" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="MarketPermissionDetailsType">
        <xs:sequence>
            <xs:element name="FirstPermissionBGDate" type="xs:date" minOccurs="0"/>
            <xs:element name="FirstPermissionBGNumber" type="xs:string" minOccurs="0"/>
            <xs:element name="FirstPermissionEUDate" type="xs:date" minOccurs="0"/>
            <xs:element name="FirstPermissionEUNumber" type="xs:string" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="RecordAppealType">
        <xs:sequence>
            <xs:element name="RecordIdentifier" type="xs:string" minOccurs="0"/>
            <xs:element name="RecordURI" type="URIType" minOccurs="0"/>
            <xs:element name="AppealDetails" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Appeal" type="AppealType" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="AppealType">
        <xs:sequence>
            <xs:element name="AppealKind" type="RecordAppealKindType" minOccurs="0"/>
            <xs:element name="DecisionDate" type="xs:date" minOccurs="0"/>
            <xs:element name="DecisionNumber" type="xs:string" minOccurs="0"/>
            <xs:element name="OppositionFilingDate" type="xs:date" minOccurs="0"/>
            <xs:element name="AppealExplanationsComment" type="xs:string" minOccurs="0"/>
            <xs:element name="AppealExplanationDocuments" type="DocumentIncludedDetailsType" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>

    <xs:simpleType name="RecordAppealKindType">
        <xs:union memberTypes="xs:token">
            <xs:simpleType>
                <xs:restriction base="xs:token">
                    <xs:enumeration value="Against refusal of registration"/>
                    <xs:enumeration value="Against decision for termination of procedure"/>
                    <xs:enumeration value="Appeal against decision of opposition"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:union>
    </xs:simpleType>


    <!-- ====================================================================== -->
    <!-- Filename    : PatentDetailsType.xsd                                    -->
    <!-- Last Update : Mon Oct 24 09:27:21 +0200 2011                           -->
    <!-- ====================================================================== -->
    <xs:complexType name="PatentDetailsType">
        <xs:sequence>
            <xs:element name="Patent" type="PatentType" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="SpcPatentDetailsType">
        <xs:sequence>
            <xs:element name="Patent" type="PatentType" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <!-- ====================================================================== -->
    <!-- Filename    : TechnicalQuestionnaireType.xsd                           -->
    <!-- ====================================================================== -->
    <xs:complexType name="TechnicalQuestionnaireType">
        <xs:sequence>
            <xs:element name="OriginSupportReproductionAbstract" type="TextType" minOccurs="0"/>
            <xs:element name="TestConditionsAbstract" type="TextType" minOccurs="0"/>
            <xs:element name="CharacteristicsAbstract" type="TextType" minOccurs="0"/>
            <xs:element name="ResistanceAbstract" type="TextType" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>


    <!-- ====================================================================== -->
    <!-- Filename    : PatentType.xsd                                           -->
    <!-- Last Update : Fri Oct 28 14:18:15 +0200 2011                           -->
    <!-- ====================================================================== -->
    <xs:complexType name="PatentType">
        <xs:sequence>
            <xs:element name="RegistrationOfficeCode" type="WIPOST3CodeType" minOccurs="0" /> <!-- SP addition: minOccurs="0" -->
            <xs:element name="IPRKind" type="IPRKindType" minOccurs="0"/>
            <xs:element name="ReceivingOfficeCode" type="WIPOST3CodeType" minOccurs="0"/>
            <xs:element name="ReceivingOfficeDate" type="xs:date" minOccurs="0"/>
            <xs:element name="ApplicationNumber" type="xs:string" minOccurs="0" />
            <xs:element name="ApplicationDate" type="xs:date" minOccurs="0" />
            <xs:element name="EntitlementDate" type="xs:date" minOccurs="0" />
            <xs:element name="ApplicationDateTime" type="xs:dateTime" minOccurs="0" />
            <xs:element name="RegistrationNumber" type="xs:string" minOccurs="0" />
            <xs:element name="RegistrationDate" type="xs:date" minOccurs="0" />
            <xs:element name="ExternalReferenceNumber" type="xs:string" minOccurs="0" />
            <xs:element name="PatentValidated" type="xs:boolean" minOccurs="0" />
            <xs:element name="RegistrationPublicationDate" type="xs:date" minOccurs="0" />
            <xs:element name="PatentURI" type="URIType" minOccurs="0" />
            <xs:element name="FilingPlace" type="xs:string" minOccurs="0"/>
            <xs:element name="ApplicationReference" type="xs:string" minOccurs="0"/>
            <xs:element name="ApplicationLanguageCode" type="ExtendedISOLanguageCodeType" minOccurs="0" /> <!-- SP edit: added minOccurs="0" -->
            <xs:element name="SecondLanguageCode" type="ExtendedISOLanguageCodeType" minOccurs="0" />
            <xs:element name="CorrespondenceLanguageCode" type="ExtendedISOLanguageCodeType" minOccurs="0" />
            <xs:element name="ExpiryDate" type="xs:date" minOccurs="0" />
            <xs:element name="TerminationDate" type="xs:date" minOccurs="0" />
            <xs:element name="PatentCurrentStatusCode" type="CurrentStatusCodeType" minOccurs="0" />
            <xs:element name="PatentGroupStatusCode" type="xs:string" minOccurs="0" />
            <xs:element name="PatentCurrentStatus" type="xs:string" minOccurs="0" />
            <xs:element name="PatentCurrentStatusDate" type="xs:date" minOccurs="0" />
            <xs:element name="PatentTitle" type="TextType" minOccurs="0"/>
            <xs:element name="PatentTitleSecondLang" type="TextType" minOccurs="0"/>
            <xs:element name="PatentAbstract" type="TextType" minOccurs="0"/>
            <xs:element name="PatentAbstractSecondLang" type="TextType" minOccurs="0"/>
            <xs:element name="PatentRepresentationSheetDetails" type="PatentRepresentationSheetDetailsType" minOccurs="0"/>
            <xs:element name="ApplicantDetails" type="ApplicantDetailsType" minOccurs="0"/>
            <xs:element name="RepresentativeDetails" type="RepresentativeDetailsType" minOccurs="0" />
            <xs:element name="InventorDetails" type="InventorDetailsType" minOccurs="0"/>
            <xs:element name="PreferredCorrespondenceKind" type="PreferredCorrespondenceKindType" minOccurs="0"/>
            <xs:element name="SVKind" type="SVKindType" minOccurs="0"/>
            <xs:element name="TitleTransliteration" type="TextType" minOccurs="0"/>
            <xs:element name="Taxon" type="TextType" minOccurs="0"/>
            <xs:element name="LatinClassification" type="TextType" minOccurs="0"/>
            <xs:element name="RegKind" type="RegKindType" minOccurs="0"/>
            <xs:element name="FirstPermissionBGNumber" type="TextType" minOccurs="0" />
            <xs:element name="FirstPermissionBGDate" type="xs:date" minOccurs="0" />
            <xs:element name="FirstPermissionBGNotificationDate" type="xs:date" minOccurs="0" />
            <xs:element name="FirstPermissionEUNumber" type="TextType" minOccurs="0" />
            <xs:element name="FirstPermissionEUDate" type="xs:date" minOccurs="0" />
            <xs:element name="CorrespondenceAddressKey" type="CorrespondenceAddressKeyType" minOccurs="0" maxOccurs="unbounded" /> <!-- SP addition: maxOccurs="unbounded" -->
            <xs:element name="CorrespondenceAddress" type="CorrespondenceAddressType" minOccurs="0" maxOccurs="unbounded" /> <!-- SP addition: maxOccurs="unbounded" -->
            <xs:element name="PriorityDetails" type="PriorityDetailsType" minOccurs="0"/>
            <xs:element name="DivisionalApplicationDetails" type="DivisionalApplicationDetailsType"	minOccurs="0" />
            <xs:element name="TransformationPriorityDetails" type="TransformationPriorityDetailsType" minOccurs="0"/>
            <xs:element name="ParallelApplication" type="ParallelApplicationDetailsType" minOccurs="0"/>
            <xs:element name="ExhibitionPriorityDetails" type="ExhibitionPriorityDetailsType" minOccurs="0"/>
            <xs:element name="PCTDetails" type="PCTDetailsType" minOccurs="0"/>

            <xs:element name="Comment" type="xs:string" minOccurs="0" />

            <xs:element name="PatentDescriptionSheetDetails" type="PatentDescriptionSheetDetailsType" minOccurs="0"/>
            <xs:element name="PatentDescriptionSheetPagesCount" type="xs:integer" minOccurs="0"/>
            <xs:element name="PatenClaimsCount" type="xs:integer" minOccurs="0"/>
            <xs:element name="PatenIndependentClaimsCount" type="xs:integer" minOccurs="0"/>
            <xs:element name="PatenDrawingsCount" type="xs:integer" minOccurs="0"/>
            <xs:element name="PatenDrawingNumber" type="xs:integer" minOccurs="0"/>
            <xs:element name="UnpublishedIndicator" type="xs:boolean" minOccurs="0" />
            <xs:element name="IPCClassDetails" type="IPCClassDetailsType" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>

    <!-- ==================================================================== -->
    <!-- IPCClassDetailsType -->
    <!-- ==================================================================== -->
    <xs:complexType name="IPCClassDetailsType">
        <xs:sequence>
            <xs:element name="IPCClass" type="xs:string" maxOccurs="unbounded" />
            <xs:element name="IPCName" type="xs:string" maxOccurs="unbounded" />
        </xs:sequence>
    </xs:complexType>

    <!-- ====================================================================== -->
    <!-- Filename    : IPRKindType.xsd                                          -->
    <!-- Last Update : Fri Oct 28 17:28:41 +0200 2011                           -->
    <!-- ====================================================================== -->
    <xs:simpleType name="IPRKindType">
        <xs:union memberTypes="xs:token">
            <xs:simpleType>
                <xs:restriction base="xs:token">
                    <xs:enumeration value="Patent"/>
                    <xs:enumeration value="Utility model"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:union>
    </xs:simpleType>

    <!-- ====================================================================== -->
    <!-- PatentRepresentationSheetDetailsType                -->
    <!-- ====================================================================== -->
    <xs:complexType name="PatentRepresentationSheetDetailsType">
        <xs:sequence>
            <xs:element name="PatentRepresentationSheet" type="PatentRepresentationSheetType" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <!-- ====================================================================== -->
    <!-- PatentRepresentationSheetType                       -->
    <!-- ====================================================================== -->
    <xs:complexType name="PatentRepresentationSheetType">
        <xs:sequence>
            <xs:element name="RepresentationSheetFilename" type="xs:string" minOccurs="0"/>
            <xs:element name="RepresentationSheetFileFormat" type="FileFormatType" minOccurs="0"/>
            <xs:element name="RepresentationSheetURI" type="URIType" minOccurs="0"/>
            <xs:element name="RepresentationSheetColourIndicator" type="xs:boolean" minOccurs="0"/>
            <xs:element name="ViewDetails" type="ViewDetailsType" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>


    <!-- ====================================================================== -->
    <!-- Filename    : ViewDetailsType.xsd                                      -->
    <!-- Last Update : Mon Oct 24 09:27:21 +0200 2011                           -->
