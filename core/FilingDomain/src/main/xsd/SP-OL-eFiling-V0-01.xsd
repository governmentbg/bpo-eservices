<?xml version='1.0' encoding='UTF-8'?>
<xs:schema elementFormDefault="unqualified" attributeFormDefault="unqualified"
           version="0.01"
           targetNamespace="http://fsp.ohim.eu/XMLSchema"
           xmlns="http://fsp.ohim.eu/XMLSchema"
           xmlns:xs="http://www.w3.org/2001/XMLSchema">

    <xs:annotation>
        <xs:documentation>
            SP Objectless Filing schema created on 2022-11-08,
            based on SP Patent Filing schema Version 0.01
        </xs:documentation>
    </xs:annotation>

    <!-- ==================================================================== -->
    <!-- External type definitions -->
    <!-- ==================================================================== -->
    <xs:include schemaLocation="external/ISOCountryCodeType-V2011.xsd" />
    <xs:include schemaLocation="external/ISOCountryCodeType-V2011.xsd" />
    <xs:include schemaLocation="external/ISOCurrencyCodeType-V2008.xsd" />
    <xs:include schemaLocation="external/ISOLanguageCodeType-V2002.xsd" />
    <xs:include schemaLocation="external/WIPOST3CodeType-V2011.xsd" />

    <!-- TRANSACTION ELEMENT -->

    <!-- ==================================================================== -->
    <!-- Transaction -->
    <!-- ==================================================================== -->
    <xs:element name="Transaction" type="TransactionType" />

    <!-- TYPES DEFINITION -->

    <!-- ==================================================================== -->
    <!-- TransactionType -->
    <!-- ==================================================================== -->
    <xs:complexType name="TransactionType">
        <xs:sequence>
            <xs:element name="TransactionHeader" type="TransactionHeaderType" minOccurs="0"/>
            <xs:element name="ObjectlessTransactionBody" type="TransactionBodyType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <!-- ====================================================================== -->
    <!-- TransactionHeaderType.xsd -->
    <!-- ====================================================================== -->
    <xs:complexType name="TransactionHeaderType">
        <xs:sequence>
            <xs:element name="SenderDetails" type="SenderDetailsType" minOccurs="0" />
            <xs:element name="ReceiverDetails" type="ReceiverDetailsType" minOccurs="0" />
        </xs:sequence>
    </xs:complexType>

    <!-- ==================================================================== -->
    <!-- TransactionBodyType -->
    <!-- ==================================================================== -->
    <xs:complexType name="TransactionBodyType">
        <xs:sequence>
            <xs:element name="TransactionErrorDetails" type="TransactionErrorDetailsType" minOccurs="0" />
            <xs:element name="TransactionContentDetails" type="TransactionContentDetailsType" />
        </xs:sequence>
    </xs:complexType>

    <!-- ====================================================================== -->
    <!-- Filename : TransactionErrorDetailsType.xsd -->
    <!-- ====================================================================== -->
    <xs:complexType name="TransactionErrorDetailsType">
        <xs:sequence>
            <xs:element name="TransactionError" type="TransactionErrorType" maxOccurs="unbounded" />
        </xs:sequence>
    </xs:complexType>

    <!-- ====================================================================== -->
    <!-- Filename : TransactionErrorType.xsd -->
    <!-- ====================================================================== -->
    <xs:complexType name="TransactionErrorType">
        <xs:sequence>
            <xs:element name="TransactionErrorCode" type="TransactionErrorCodeType" minOccurs="0" />
            <xs:element name="TransactionErrorText" type="TextType" minOccurs="0" />
        </xs:sequence>
    </xs:complexType>

    <!-- ==================================================================== -->
    <!-- TransactionDataType -->
    <!-- ==================================================================== -->
    <xs:complexType name="TransactionDataType">
        <xs:sequence>
            <!-- SP additional elements -->
            <xs:element name="ObjectlessServiceApplication" type="ObjectlessServicesApplicationType"	minOccurs="0" />
            <xs:element name="PaymentDetails" type="PaymentDetailsType" minOccurs="0" />
        </xs:sequence>
    </xs:complexType>



    <!-- ==================================================================== -->
    <!-- PatentServicesApplicationType -->
    <!-- ==================================================================== -->
    <xs:complexType name="ObjectlessServicesApplicationType">
        <xs:sequence>
            <!-- Standard Application fields -->
            <xs:element name="RequestSoftware" type="RequestSoftwareType" minOccurs="0"/>
            <xs:element name="ApplicationFormName" type="xs:string" minOccurs="0"/>

            <xs:element name="RequestExamination" type="RequestExaminationType" minOccurs="0"/>
            <xs:element name="DocumentIncludedDetails" type="DocumentIncludedDetailsType" minOccurs="0"/>
            <xs:element name="PaymentDetails" type="PaymentDetailsType" minOccurs="0"/>
            <xs:element name="ReimbursementDetails" type="ReimbursementDetailsType" minOccurs="0"/>
            <xs:element name="SignatoryDetails" type="SignatoryDetailsType" minOccurs="0"/>
            <!-- Generic Filing information -->
            <xs:element name="ApplicationFilingNumber" type="xs:string" minOccurs="0" />
            <xs:element name="ApplicationFilingDate" type="xs:date" minOccurs="0" />
            <xs:element name="ApplicationFilingDateTime" type="xs:dateTime" minOccurs="0" />
            <xs:element name="WarningMessages" type="xs:string" minOccurs="0" maxOccurs="unbounded" />
            <xs:element name="Comment" type="xs:string" minOccurs="0" />
            <!-- Generic Application information -->
            <xs:element name="ReceivingOfficeCode" type="WIPOST3CodeType" minOccurs="0" />
            <xs:element name="ReceivingOfficeDate" type="xs:date" minOccurs="0" />
            <xs:element name="ApplicationLanguageCode" type="ExtendedISOLanguageCodeType" minOccurs="0"/>
            <xs:element name="ApplicationSecondLanguageCode" type="ExtendedISOLanguageCodeType" minOccurs="0" />
            <xs:element name="CorrespondenceLanguageCode" type="ExtendedISOLanguageCodeType" minOccurs="0"/>
            <xs:element name="ApplicationNumber" type="xs:string" minOccurs="0" />
            <xs:element name="ApplicationDate" type="xs:date" minOccurs="0" />
            <xs:element name="ExpirationDate" type="xs:date" minOccurs="0" />
            <xs:element name="ApplicationDateTime" type="xs:dateTime" minOccurs="0" />
            <xs:element name="ApplicationReference" type="xs:string" minOccurs="0" />
            <xs:element name="CorrespondenceAddress" type="CorrespondenceAddressType" minOccurs="0" maxOccurs="unbounded" />
            <xs:element name="ApplicationRequestType" type="xs:string" minOccurs="0" />
            <!-- Application persons -->
            <xs:element name="ApplicantDetails" type="ApplicantDetailsType" />
            <xs:element name="RepresentativeDetails" type="RepresentativeDetailsType" minOccurs="0" />

            <xs:element name="EsignDocDeclarationIndicator" type="xs:boolean" minOccurs="0"/>
            <xs:element name="ProcessInitiatedBeforePublicationIndicator" type="xs:boolean" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>

    <!-- ==================================================================== -->
    <!-- ApplicantDetailsType -->
    <!-- ==================================================================== -->
    <xs:complexType name="ApplicantDetailsType">
        <xs:sequence>
            <xs:element name="ApplicantKey" type="KeyType" minOccurs="0" maxOccurs="unbounded" />
            <xs:element name="Applicant" type="ApplicantType" minOccurs="0" maxOccurs="unbounded" />
        </xs:sequence>
    </xs:complexType>

    <!-- ==================================================================== -->
    <!-- ApplicantType -->
    <!-- ==================================================================== -->
    <xs:complexType name="ApplicantType">
        <xs:sequence>
            <xs:element name="ApplicantIdentifier" type="IdentifierType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="ApplicantURI" type="URIType" minOccurs="0"/>
            <xs:element name="ApplicantReference" type="xs:string" minOccurs="0"/>
            <xs:element name="ApplicantSequenceNumber" type="xs:integer" minOccurs="0"/>
            <xs:element name="ApplicantNationalityCode" type="NationalityCodeType" minOccurs="0"/>
            <xs:element name="ApplicantLegalEntity" type="xs:string" minOccurs="0"/>
            <xs:element name="ApplicantIncorporationCountryCode" type="ISOCountryCodeType" minOccurs="0"/>
            <xs:element name="ApplicantIncorporationState" type="xs:string" minOccurs="0"/>
            <xs:element name="Comment" type="TextType" minOccurs="0"/>
            <xs:element name="PreferredCorrespondenceKind" type="PreferredCorrespondenceKindType" minOccurs="0"/>
            <xs:element name="ApplicantDomicileLocality" type="xs:token" minOccurs="0"/>
            <xs:element name="ApplicantAddressBook" type="AddressBookType" minOccurs="0"/>
            <xs:element name="ApplicantEntitlement" type="EntitlementType" minOccurs="0"/>
            <xs:element name="ApplicantVersion" type="VersionType" minOccurs="0"/>
            <!-- SP additional elements -->
            <xs:element name="ApplicantCorrespondenceAddressBook" type="AddressBookType" minOccurs="0" maxOccurs="unbounded" />
            <xs:element name="ApplicantDomicileCountry" type="ISOCountryCodeType" minOccurs="0" />
            <xs:element name="ApplicantIsOwner" type="xs:boolean" minOccurs="0" />
            <xs:element name="EntitlementToApply" type="xs:boolean" minOccurs="0" />
            <xs:element name="ApplicantIsInventor" type="xs:boolean" minOccurs="0" />
        </xs:sequence>
        <xs:attribute name="operationCode" type="OperationCodeType" use="optional"/>
    </xs:complexType>

    <!-- ==================================================================== -->
    <!-- RepresentativeDetailsType -->
    <!-- ==================================================================== -->
    <xs:complexType name="RepresentativeDetailsType">
        <xs:sequence>
            <xs:element name="RepresentativeKey" type="KeyType"	minOccurs="0" maxOccurs="unbounded" />
            <xs:element name="Representative" type="RepresentativeType"	minOccurs="0" maxOccurs="unbounded" />
        </xs:sequence>
    </xs:complexType>

    <!-- ==================================================================== -->
    <!-- RepresentativeRelationshipDetailsType -->
    <!-- ==================================================================== -->
    <xs:complexType name="RepresentativeRelationshipDetailsType">
        <xs:sequence>
            <xs:element name="RepresentativeRelationship" type="PartyRelationshipType" maxOccurs="unbounded" />
        </xs:sequence>
    </xs:complexType>

    <!-- ==================================================================== -->
    <!-- RepresentativeType -->
    <!-- ==================================================================== -->
    <xs:complexType name="RepresentativeType">
        <xs:sequence>
            <xs:element name="RepresentativeIdentifier" type="IdentifierType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="RepresentativeURI" type="URIType" minOccurs="0"/>
            <xs:element name="RepresentativeReference" type="xs:string" minOccurs="0"/>
            <xs:element name="RepresentativeSequenceNumber" type="xs:integer" minOccurs="0"/>
            <xs:element name="RepresentativeKindCode" type="RepresentativeKindCodeType" minOccurs="0"/>
            <xs:element name="RepresentativeNationalityCode" type="NationalityCodeType" minOccurs="0"/>
            <xs:element name="ExclusiveForAct" type="xs:boolean" minOccurs="0"/>
            <xs:element name="RepresentativeLegalEntity" type="xs:string" minOccurs="0"/>
            <xs:element name="RepresentativeIncorporationCountryCode" type="ISOCountryCodeType" minOccurs="0"/>
            <xs:element name="RepresentativeIncorporationState" type="xs:string" minOccurs="0"/>
            <xs:element name="Comment" type="TextType" minOccurs="0"/>
            <xs:element name="PreferredCorrespondenceKind" type="PreferredCorrespondenceKindType" minOccurs="0"/>
            <xs:element name="RepresentativeAddressBook" type="AddressBookType" minOccurs="0"/>
            <xs:element name="RepresentativeRelationshipDetails" type="RepresentativeRelationshipDetailsType" minOccurs="0"/>
            <xs:element name="RepresentativeVersion" type="VersionType" minOccurs="0"/>
            <!-- SP additional elements -->
            <xs:element name="RepresentativeCorrespondenceAddressBook" type="AddressBookType" minOccurs="0" maxOccurs="unbounded" />
            <xs:element name="RepresentativeDomicileLocality" type="xs:token" minOccurs="0" />
            <xs:element name="RepresentativeDomicileCountry" type="ISOCountryCodeType" minOccurs="0" />
            <xs:element name="RepresentativeEconomicConnection" type="xs:string" minOccurs="0" />
            <xs:element name="RepresentativeDocumentDetails" type="DocumentIncludedDetailsType" minOccurs="0"/>
            <xs:element name="RepresentedPartyKindCode" type="RepresentedPartyKindCodeType" minOccurs="0" />
            <xs:element name="RepresentedPartyKindText" type="TextType" minOccurs="0" />
            <xs:element name="PowValidityEndDate" type="xs:date" minOccurs="0" />
            <xs:element name="PowReauthorizationIndicator" type="xs:boolean" minOccurs="0" />
            <xs:element name="PowValidityIndefiniteIndicator" type="xs:boolean" minOccurs="0" />
            <xs:element name="PowRevokesPreviousIndicator" type="xs:boolean" minOccurs="0" />
            <xs:element name="PowNote" type="xs:string" minOccurs="0" />
        </xs:sequence>
        <xs:attribute name="operationCode" type="OperationCodeType" use="optional"/>
    </xs:complexType>

    <!-- ====================================================================== -->
    <!-- Filename    : RepresentedPartyKindCodeType.xsd                                     -->
    <!-- ====================================================================== -->
    <xs:simpleType name="RepresentedPartyKindCodeType">
        <xs:union memberTypes="xs:token">
            <xs:simpleType>
                <xs:restriction base="xs:token">
                    <xs:enumeration value="Applicant"/>
                    <xs:enumeration value="Owner"/>
                    <xs:enumeration value="Assignee"/>
                    <xs:enumeration value="Other"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:union>
    </xs:simpleType>

    <!-- ==================================================================== -->
    <!-- DocumentIncludedDetailsType -->
    <!-- ==================================================================== -->
    <xs:complexType name="DocumentIncludedDetailsType">
        <xs:sequence>
            <xs:element name="DocumentIncluded" type="DocumentType"	maxOccurs="unbounded" />

            <xs:element name="TrueDocumentsIndicator" type="xs:boolean" minOccurs="0" />
        </xs:sequence>
    </xs:complexType>

    <!-- ====================================================================== -->
    <!-- Filename    : DocumentType.xsd                                         -->
    <!-- Last Update : Mon Oct 24 09:49:41 +0200 2011                           -->
    <!-- ====================================================================== -->
    <xs:complexType name="DocumentType">
        <xs:sequence>
            <xs:element name="DocumentName" type="xs:string" minOccurs="0"/>
            <xs:element name="DocumentURI" type="URIType" minOccurs="0"/>
            <xs:element name="DocumentFilename" type="xs:string" minOccurs="0"/>
            <xs:element name="DocumentFileFormat" type="FileFormatType" minOccurs="0"/>
            <xs:element name="DocumentDate" type="xs:date" minOccurs="0"/>
            <xs:element name="DocumentLanguageCode" type="ISOLanguageCodeType" minOccurs="0"/>
            <xs:element name="DocumentMedia" type="DocumentMediaType" minOccurs="0"/>
            <xs:element name="DocumentLocation" type="xs:string" minOccurs="0"/>
            <xs:element name="DocumentVersion" type="xs:string" minOccurs="0"/>
            <xs:element name="DocumentSizeInByte" type="xs:integer" minOccurs="0"/>
            <xs:element name="Comment" type="TextType" minOccurs="0"/>
            <xs:element name="DocumentBinary" type="xs:base64Binary" minOccurs="0"/>
            <!-- SP additional fields -->
            <xs:element name="DocumentKind" type="DocumentKindType"	minOccurs="0" />
            <xs:element name="DocumentStatus" type="DocumentStatusCodeType"	minOccurs="0" />
            <xs:element name="DocumentIdentifier" type="xs:string" />
        </xs:sequence>
    </xs:complexType>

    <!-- ==================================================================== -->
    <!-- DocumentKindType -->
    <!-- ==================================================================== -->
    <xs:simpleType name="DocumentKindType">
        <xs:restriction base="xs:token">
            <xs:enumeration value="Priority Certificate" />
            <xs:enumeration value="Priority Certificate Translation" />
            <xs:enumeration value="Application Receipt" />
            <xs:enumeration value="Proof of Payment" />
            <xs:enumeration value="Signed Declaration" />
            <xs:enumeration value="Power Of Attorney" />
            <xs:enumeration value="PCT Copy" />
            <xs:enumeration value="Exhibition Priority Certificate" />
            <xs:enumeration value="Small Company Declaration" />
            <xs:enumeration value="Declaration For University" />
            <xs:enumeration value="Request For Examination Document" />
            <xs:enumeration value="EPO Bulletin Publication Excerpt" />
            <xs:enumeration value="EPO Decision Copy" />
            <xs:enumeration value="EPO Decision Translation" />
            <xs:enumeration value="EPO Transfer Change Form Copy" />
            <xs:enumeration value="EPO Transfer Change Form Translation" />
            <xs:enumeration value="Other Document" />
            <xs:enumeration value="Parallel Application Declaration" />
            <xs:enumeration value="Explanation Of Grounds" />

            <xs:enumeration value="Commercial Register Excerpt"/>
            <xs:enumeration value="Commercial Register Excerpt Translation"/>
            <xs:enumeration value="Current Status Certificate"/>
            <xs:enumeration value="Transfer Proof"/>
            <xs:enumeration value="Licence Proof"/>
            <xs:enumeration value="Licence Contract Annex"/>
            <xs:enumeration value="Rem Proof"/>
            <xs:enumeration value="Previous Creditor Consent"/>
            <xs:enumeration value="Pledgor Consent"/>
            <xs:enumeration value="Declaration ZOZ"/>
            <xs:enumeration value="Declaration DOPK"/>
            <xs:enumeration value="Confirmation Registration CROZ"/>
            <xs:enumeration value="Security Proof"/>
            <xs:enumeration value="Bankruptcy Proof"/>
            <xs:enumeration value="Request"/>
            <xs:enumeration value="Objection"/>
            <xs:enumeration value="Official Position"/>
            <xs:enumeration value="Response"/>
            <xs:enumeration value="Correction Letter"/>
            <xs:enumeration value="Translation Correction"/>
            <xs:enumeration value="Action Correction"/>
            <xs:enumeration value="EUIPO Communication Copy"/>
            <xs:enumeration value="Surrender Declaration"/>
            <xs:enumeration value="Request Change Legal Status"/>
            <xs:enumeration value="Evidence"/>
            <xs:enumeration value="Mark Registration Certificate"/>
            <xs:enumeration value="Inquiry"/>
            <xs:enumeration value="Description"/>
            <xs:enumeration value="Claim"/>
            <xs:enumeration value="Drawing"/>

            <xs:enumeration value="Permit Market BG"/>
            <xs:enumeration value="Permit Market EU"/>
            <xs:enumeration value="Authorization Market EU"/>

            <xs:enumeration value="First Usage Declaration"/>
            <xs:enumeration value="Trading Secret Guides"/>
            <xs:enumeration value="Entitlement To Apply Declaration"/>
            <xs:enumeration value="Integral Schema Sample"/>
        </xs:restriction>
    </xs:simpleType>

    <!-- ==================================================================== -->
    <!-- DocumentStatusCodeType -->
    <!-- ==================================================================== -->
    <xs:simpleType name="DocumentStatusCodeType">
        <xs:restriction base="xs:token">
            <xs:enumeration value="Attached" />
            <xs:enumeration value="To Follow" />
            <xs:enumeration value="Provided" />
        </xs:restriction>
    </xs:simpleType>


    <!-- ====================================================================== -->
    <!-- Filename    : DocumentMediaType.xsd                                    -->
    <!-- Last Update : Mon Oct 24 09:49:41 +0200 2011                           -->
    <!-- ====================================================================== -->
    <xs:simpleType name="DocumentMediaType">
        <xs:union memberTypes="xs:token">
            <xs:simpleType>
                <xs:restriction base="xs:token">
                    <xs:enumeration value="Paper"/>
                    <xs:enumeration value="Fax"/>
                    <xs:enumeration value="File"/>
                    <xs:enumeration value="Other"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:union>
    </xs:simpleType>

    <!-- ==================================================================== -->
    <!-- PaymentDetailsType -->
    <!-- ==================================================================== -->
    <xs:complexType name="PaymentDetailsType">
        <xs:sequence>
            <xs:element name="Payment" type="PaymentType" maxOccurs="unbounded" />
        </xs:sequence>
    </xs:complexType>

    <!-- ==================================================================== -->
    <!-- PaymentFeeDetailsType -->
    <!-- ==================================================================== -->
    <xs:complexType name="PaymentFeeDetailsType">
        <xs:sequence>
            <xs:element name="PaymentFee" type="PaymentFeeType"	maxOccurs="unbounded" />
        </xs:sequence>
    </xs:complexType>

    <!-- ==================================================================== -->
    <!-- PaymentFeeType -->
    <!-- ==================================================================== -->
    <xs:complexType name="PaymentFeeType">
        <xs:sequence>
            <xs:element name="FeeIdentifier" type="xs:string" minOccurs="0" />
            <xs:element name="FeeReference" type="xs:string" minOccurs="0" />
            <xs:element name="FeeAmount" type="AmountType" />
            <xs:element name="FeeUnitQuantity" type="xs:decimal" minOccurs="0" />
            <xs:element name="FeeUnitAmount" type="AmountType" minOccurs="0" />
            <xs:element name="Comment" type="TextType" minOccurs="0" />
            <!--     These fields are for UM_RENEWAL       -->
            <xs:element name="ExpirationExtentYearsFromEntitlement" type="xs:string" minOccurs="0" />
            <xs:element name="ExpirationExtentNewDate" type="xs:date" minOccurs="0" />
        </xs:sequence>
    </xs:complexType>

    <!-- ==================================================================== -->
    <!-- PaymentMethodType -->
    <!-- ==================================================================== -->
    <xs:complexType name="PaymentMethodType">
        <xs:sequence>
            <xs:element name="Account" type="AccountType" minOccurs="0" />
            <xs:element name="CardAccount" type="CardAccountType" minOccurs="0" />
            <xs:element name="BankTransfer" type="BankTransferType"	minOccurs="0" />
        </xs:sequence>
    </xs:complexType>

    <!-- ==================================================================== -->
    <!-- BankTransferType -->
    <!-- ==================================================================== -->
    <xs:complexType name="BankTransferType">
        <xs:sequence>
            <xs:element name="BankTransferIdentifier" type="xs:string" />
            <xs:element name="BankTransferDate" type="xs:date" />
            <xs:element name="OriginBankName" type="xs:string" minOccurs="0" />
            <xs:element name="BankDestinationAccount" type="xs:string" />
        </xs:sequence>
    </xs:complexType>

    <!-- ==================================================================== -->
    <!-- CardAccountType -->
    <!-- ==================================================================== -->
    <xs:complexType name="CardAccountType">
        <xs:sequence>
            <xs:element name="CardPrimaryAccountNumber" type="xs:string" />
            <xs:element name="CardNetworkIdentifier" type="xs:string" />
            <xs:element name="CardKindCode" type="CardKindCodeType"	minOccurs="0" />
            <xs:element name="CardCustomerIdentifier" type="xs:string" minOccurs="0" />
            <xs:element name="CardValidityStartDate" type="xs:date"	minOccurs="0" />
            <xs:element name="CardExpiryDate" type="xs:date" minOccurs="0" />
            <xs:element name="CardIssuerIdentifier" type="xs:string" minOccurs="0" />
            <xs:element name="CardIssueNumber" type="xs:string"	minOccurs="0" />
            <xs:element name="CardCV2Identifier" type="xs:string" minOccurs="0" />
            <xs:element name="CardChipCode" type="ChipCodeType"	minOccurs="0" />
            <xs:element name="CardChipApplicationIdentifier" type="xs:string" minOccurs="0" />
            <xs:element name="CardHolderName" type="TextType" minOccurs="0" />
        </xs:sequence>
    </xs:complexType>

    <!-- ====================================================================== -->
    <!-- Filename : ChipCodeType.xsd -->
    <!-- ====================================================================== -->
    <xs:simpleType name="ChipCodeType">
        <xs:restriction base="xs:token">
            <xs:enumeration value="Chip" />
            <xs:enumeration value="Magnetic Stripe" />
        </xs:restriction>
    </xs:simpleType>

    <!-- ====================================================================== -->
    <!-- Filename : CardKindCodeType.xsd -->
    <!-- ====================================================================== -->
    <xs:simpleType name="CardKindCodeType">
        <xs:restriction base="xs:token">
            <xs:enumeration value="Credit Card" />
            <xs:enumeration value="Debit Card" />
        </xs:restriction>
    </xs:simpleType>

    <!-- ==================================================================== -->
    <!-- AccountType -->
    <!-- ==================================================================== -->
    <xs:complexType name="AccountType">
        <xs:sequence>
            <xs:element name="AccountBank" type="xs:string" />
            <xs:element name="AccountIdentifier" type="xs:string" />
            <xs:element name="AccountKind" type="xs:string" minOccurs="0" />
            <xs:element name="AccountHolderName" type="TextType" minOccurs="0" />
            <xs:element name="AccountDebitKind" type="AccountDebitKindType" minOccurs="0" />
        </xs:sequence>
    </xs:complexType>

    <!-- ==================================================================== -->
    <!-- AccountDebitKindType -->
    <!-- ==================================================================== -->
    <xs:simpleType name="AccountDebitKindType">
        <xs:restriction base="xs:token">
            <xs:enumeration value="Basic Fee Immediate" />
            <xs:enumeration value="Basic Fee End of Period" />
            <xs:enumeration value="Basic and Class Fee Immediate" />
            <xs:enumeration value="Basic and Class Fee End of Period"/>
            <xs:enumeration value="Do not use Current Account"/>
        </xs:restriction>
    </xs:simpleType>

    <!-- ==================================================================== -->
    <!-- PaymentType -->
    <!-- ==================================================================== -->
    <xs:complexType name="PaymentType">
        <xs:sequence>
            <xs:element name="PaymentIdentifier" type="xs:string" minOccurs="0" /> <!-- SP addition: minOccurs="0" -->
            <xs:element name="PaymentURI" type="URIType" minOccurs="0" />
            <xs:element name="PaymentReference" type="xs:string" minOccurs="0" />
            <xs:element name="PayerName" type="NameType" minOccurs="0" />
            <xs:element name="PaymentStatus" type="PaymentStatusCodeType" />
            <xs:element name="Comment" type="TextType" minOccurs="0" />
            <xs:element name="PaymentDate" type="xs:date" minOccurs="0" />
            <xs:element name="PaymentMethod" type="PaymentMethodType" minOccurs="0" />
            <xs:element name="PaymentFeeDetails" type="PaymentFeeDetailsType" minOccurs="0" />
            <!-- SP additional fields -->
            <xs:element name="PayerIdentifier" type="RoleType" minOccurs="0" />
            <xs:element name="PayerAddressBook" type="AddressBookType" minOccurs="0" />
            <xs:element name="PaymentKind" type="PaymentKindType" minOccurs="0" />
            <xs:element name="PaymentDueDate" type="xs:date" minOccurs="0" />
            <xs:element name="PaymentDocument" type="DocumentType" minOccurs="0" maxOccurs="unbounded" />
        </xs:sequence>
    </xs:complexType>

    <!-- ==================================================================== -->
    <!-- PaymentExternalKindType -->
    <!-- ==================================================================== -->
    <xs:simpleType name="PaymentKindType">
        <xs:restriction base="xs:token">
            <xs:enumeration value="Credit Card" />
            <xs:enumeration value="Bank Transfer" />
            <xs:enumeration value="Debit Card" />
            <xs:enumeration value="Online Payment" />
            <xs:enumeration value="Postal Order" />
            <xs:enumeration value="Current Account" />
            <xs:enumeration value="Other" />
        </xs:restriction>
    </xs:simpleType>

    <!-- ==================================================================== -->
    <!-- PaymentStatusCodeType -->
    <!-- ==================================================================== -->
    <xs:simpleType name="PaymentStatusCodeType">
        <xs:restriction base="xs:token">
            <xs:enumeration value="Done" />
            <xs:enumeration value="Attached" />
            <xs:enumeration value="To follow" />
            <xs:enumeration value="Bank transfer to follow" />
            <xs:enumeration value="Period of Grace Requested" />
            <xs:enumeration value="Undefined" />
        </xs:restriction>
    </xs:simpleType>

    <!-- ==================================================================== -->
    <!-- AmountType -->
    <!-- ==================================================================== -->
    <xs:complexType name="AmountType">
        <xs:simpleContent>
            <xs:extension base="xs:decimal">
