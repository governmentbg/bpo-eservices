<mappings xmlns="http://dozer.sourceforge.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://dozer.sourceforge.net http://dozer.sourceforge.net/schema/beanmapping.xsd">
	<configuration>
		<custom-converters>
			<converter type="eu.ohim.sp.core.domain.converter.EmailConverter">
				<class-a>java.lang.String</class-a>
				<class-b>eu.ohim.sp.core.domain.contact.Email</class-b>
			</converter>
			<converter type="eu.ohim.sp.core.domain.converter.FSPEnumConverter" >
				<class-a>java.lang.Enum</class-a>
				<class-b>java.lang.Enum</class-b>
			</converter>
			<converter type="eu.ohim.sp.core.domain.converter.FSPEnumConverter">
				<class-a>java.lang.Enum</class-a>
				<class-b>java.lang.String</class-b>
			</converter>
            <converter type="eu.ohim.sp.core.domain.converter.FSPEnumConverter">
                <class-a>eu.ohim.sp.filing.domain.ds.ExtendedISOLanguageCode</class-a>
                <class-b>java.lang.String</class-b>
            </converter>
            <converter type="eu.ohim.sp.core.domain.converter.FSPEnumConverter">
                <class-a>eu.ohim.sp.filing.domain.ds.ISOLanguageCode</class-a>
                <class-b>java.lang.String</class-b>
            </converter>
			<converter type="eu.ohim.sp.core.domain.converter.FSPEnumConverter">
				<class-a>eu.ohim.sp.filing.domain.ds.ExtendedWIPOST3Code</class-a>
				<class-b>java.lang.String</class-b>
			</converter>
			<converter type="eu.ohim.sp.core.domain.converter.FSPEnumConverter">
				<class-a>eu.ohim.sp.filing.domain.ds.FileFormat</class-a>
				<class-b>java.lang.String</class-b>
			</converter>
			<converter type="eu.ohim.sp.core.domain.converter.TextConverter">
				<class-a>java.lang.String</class-a>
				<class-b>eu.ohim.sp.filing.domain.ds.Text</class-b>
			</converter>
            <converter type="eu.ohim.sp.core.domain.converter.ds.GoodsServicesDetailsConverter">
                <class-a>eu.ohim.sp.core.domain.trademark.ClassDescription</class-a>
                <class-b>eu.ohim.sp.filing.domain.ds.TMClassDescriptionType</class-b>
            </converter>
            <converter type="eu.ohim.sp.core.domain.converter.ds.MarkImageConverter">
                <class-a>eu.ohim.sp.core.domain.trademark.ImageSpecification</class-a>
                <class-b>eu.ohim.sp.filing.domain.ds.MarkImageType</class-b>
            </converter>
			<converter type="eu.ohim.sp.core.domain.converter.FSPEnumConverter">
				<class-a>eu.ohim.sp.filing.domain.ds.WIPOST3Code</class-a>
				<class-b>java.lang.String</class-b>
			</converter>
            <converter type="eu.ohim.sp.core.domain.converter.PhoneConverter">
                <class-a>eu.ohim.sp.filing.domain.ds.Phone</class-a>
                <class-b>eu.ohim.sp.core.domain.contact.Phone</class-b>
            </converter>
            <converter type="eu.ohim.sp.core.domain.converter.PhoneConverter">
                <class-a>eu.ohim.sp.core.domain.contact.Phone</class-a>
                <class-b>java.lang.String</class-b>
            </converter>
            <converter type="eu.ohim.sp.core.domain.converter.ds.StringToViewRepresentationSizeConverter">
                <class-a>java.lang.String</class-a>
                <class-b>eu.ohim.sp.filing.domain.ds.ViewRepresentationSizeType</class-b>
            </converter>
            <converter type="eu.ohim.sp.core.domain.converter.ds.ProductIndicationToClassDescriptionConverter">
                <class-a>eu.ohim.sp.core.domain.design.ProductIndicationClass</class-a>
                <class-b>eu.ohim.sp.filing.domain.ds.ClassDescription</class-b>
            </converter>
            <converter type="eu.ohim.sp.core.domain.converter.ds.ApplicationTransactionConverter">
                <class-a>eu.ohim.sp.core.domain.design.DesignApplication</class-a>
                <class-b>eu.ohim.sp.filing.domain.ds.Transaction</class-b>
            </converter>
            <converter type="eu.ohim.sp.core.domain.converter.ds.DesignEservicesTransactionConverter">
                <class-a>eu.ohim.sp.core.domain.design.DSeServiceApplication</class-a>
                <class-b>eu.ohim.sp.filing.domain.ds.Transaction</class-b>
            </converter>
            <converter type="eu.ohim.sp.core.domain.converter.ds.EntitlementKindEnumConverter">
                <class-a>eu.ohim.sp.core.domain.application.EntitlementKind</class-a>
                <class-b>java.lang.String</class-b>
            </converter>

        </custom-converters>
	</configuration>

    <mapping map-null="false">
        <class-a>eu.ohim.sp.core.domain.resources.AttachedDocument</class-a>
        <class-b>eu.ohim.sp.filing.domain.ds.Document</class-b>
        <field>
            <a>document</a>
            <b>this</b>
        </field>
        <field>
            <a>document.comment</a>
            <b>comment.value</b>
        </field>
        <field>
            <a>document.uri</a>
            <b>uri.value</b>
        </field>
        <field>
            <a get-method="getDocumentId" set-method="setDocumentId">document.documentId</a>
            <b>documentId</b>
        </field>
        <field>
            <a>document.fileSize</a>
            <b>fileSize</b>
        </field>
        <field custom-converter="eu.ohim.sp.core.domain.converter.FSPEnumConverter">
            <a>documentKind</a>
            <b>kind</b>
            <a-hint>java.lang.String</a-hint>
            <b-hint>java.lang.Enum</b-hint>
        </field>
        <field custom-converter="eu.ohim.sp.core.domain.converter.FSPEnumConverter">
            <a>document.fileFormat</a>
            <b>documentFileFormat</b>
            <a-hint>java.lang.String</a-hint>
            <b-hint>java.lang.Enum</b-hint>
        </field>
        <field custom-converter="eu.ohim.sp.core.domain.converter.FSPEnumConverter">
            <a get-method="getStatus" set-method="setStatus">document.status</a>
            <b>status</b>
            <a-hint>java.lang.String</a-hint>
            <b-hint>java.lang.Enum</b-hint>
        </field>
    </mapping>

</mappings>
