    <p:fileUpload fieldName="fileWrapperImage">
        <p:fileUploadInfo>
            <p:description>true</p:description>
            <p:maximumFiles>1</p:maximumFiles>
            <p:labelCode>p:labelCode</p:labelCode>
            <p:allowedTypes>
                <p:allowedFileType allowedSize="2097152" value="image/jpeg" description="JPEG">
                    <p:imageValidationParameters>
                        <p:minSizePixels height="800" width="600"/>
                        <p:maxSizePixels height="2010" width="2835"/>
                        <p:maxSizeCentimeters height="17" width="24"/>
                        <p:exifInfo>
                            <p:validResolutionUnits>
                                <p:validByte>0</p:validByte>
                                <p:validByte>1</p:validByte>
                                <p:validByte>2</p:validByte>
                            </p:validResolutionUnits>
                            <p:minXDensity>
                                300
                            </p:minXDensity>
                            <p:minYDensity>
                                300
                            </p:minYDensity>
                        </p:exifInfo>
                        <p:acceptedColorModels>
                            <p:colorModel>RGB</p:colorModel>
                            <p:colorModel>Grayscale</p:colorModel>
                            <p:colorModel>BW</p:colorModel>
                        </p:acceptedColorModels>
                    </p:imageValidationParameters>
                </p:allowedFileType>
                <p:allowedFileType allowedSize="2097152" value="image/pjpeg">
                    <p:imageValidationParameters>
                        <p:minSizePixels height="800" width="600"/>
                        <p:maxSizePixels height="2010" width="2835"/>
                        <p:maxSizeCentimeters height="17" width="24"/>
                        <p:exifInfo>
                            <p:validResolutionUnits>
                                <p:validByte>0</p:validByte>
                                <p:validByte>1</p:validByte>
                                <p:validByte>2</p:validByte>
                            </p:validResolutionUnits>
                            <p:minXDensity>
                                300
                            </p:minXDensity>
                            <p:minYDensity>
                                300
                            </p:minYDensity>
                        </p:exifInfo>
                        <p:acceptedColorModels>
                            <p:colorModel>RGB</p:colorModel>
                            <p:colorModel>Grayscale</p:colorModel>
                            <p:colorModel>BW</p:colorModel>
                        </p:acceptedColorModels>
                    </p:imageValidationParameters>
                </p:allowedFileType>
            </p:allowedTypes>
        </p:fileUploadInfo>
    </p:fileUpload>
    <p:fileUpload fieldName="soundFile">
        <p:fileUploadInfo>
            <p:description>true</p:description>
            <p:maximumFiles>1</p:maximumFiles>
            <p:labelCode>p:labelCode</p:labelCode>
            <p:allowedTypes>
                <p:allowedFileType allowedSize="2097152" value="audio/mp3">
 					<p:soundValidationParameters>
                        <p:minSampleRate>22050</p:minSampleRate>
                        <p:allowedSampleRates>
                            <p:sampleRate>8000</p:sampleRate>
                            <p:sampleRate>11025</p:sampleRate>
                            <p:sampleRate>22050</p:sampleRate>
                            <p:sampleRate>44100</p:sampleRate>
                        </p:allowedSampleRates>
                        <p:channels>
                            <p:channel>1</p:channel>
                            <p:channel>2</p:channel>
                        </p:channels>
                    </p:soundValidationParameters>                
                </p:allowedFileType>
                <p:allowedFileType allowedSize="2097152" value="audio/mp3">
 					<p:soundValidationParameters>
                        <p:minSampleRate>22050</p:minSampleRate>
                        <p:allowedSampleRates>
                            <p:sampleRate>8000</p:sampleRate>
                            <p:sampleRate>11025</p:sampleRate>
                            <p:sampleRate>22050</p:sampleRate>
                            <p:sampleRate>44100</p:sampleRate>
                        </p:allowedSampleRates>
                        <p:channels>
                            <p:channel>1</p:channel>
                            <p:channel>2</p:channel>
                        </p:channels>
                    </p:soundValidationParameters>					                
                </p:allowedFileType>
                <p:allowedFileType allowedSize="2097152" value="audio/mp3" description="MP3" sound="true">
                    <p:soundValidationParameters>
                        <p:minSampleRate>22050</p:minSampleRate>
                        <p:allowedSampleRates>
                            <p:sampleRate>8000</p:sampleRate>
                            <p:sampleRate>11025</p:sampleRate>
                            <p:sampleRate>22050</p:sampleRate>
                            <p:sampleRate>44100</p:sampleRate>
                        </p:allowedSampleRates>
                        <p:channels>
                            <p:channel>1</p:channel>
                            <p:channel>2</p:channel>
                        </p:channels>
                    </p:soundValidationParameters>
                </p:allowedFileType>
            </p:allowedTypes>
        </p:fileUploadInfo>
    </p:fileUpload>
