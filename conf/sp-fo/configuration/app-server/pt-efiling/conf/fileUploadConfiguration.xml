    </p:fileUpload>
    <p:fileUpload fieldName="smallCompanyFiles">
        <p:fileUploadInfo>
            <p:description>false</p:description>
            <p:maximumFiles>1</p:maximumFiles>
            <p:labelCode>p:labelCode</p:labelCode>
            <p:allowedTypes>
                <p:allowedFileType allowedSize="10485760" value="application/pdf" description="PDF"/>
            </p:allowedTypes>
        </p:fileUploadInfo>
    </p:fileUpload>
    <p:fileUpload fieldName="postponementOfPublicationFiles">
        <p:fileUploadInfo>
            <p:description>false</p:description>
            <p:maximumFiles>1</p:maximumFiles>
            <p:labelCode>p:labelCode</p:labelCode>
            <p:allowedTypes>
                <p:allowedFileType allowedSize="10485760" value="application/pdf" description="PDF"/>
