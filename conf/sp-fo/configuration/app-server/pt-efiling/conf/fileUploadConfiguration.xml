            <p:allowedTypes>
                <p:allowedFileType allowedSize="52428800" value="application/pdf" description="PDF"/>
            </p:allowedTypes>
        </p:fileUploadInfo>
    </p:fileUpload>
    <p:fileUpload fieldName="patentDrawings">
        <p:fileUploadInfo>
            <p:description>false</p:description>
            <p:maximumFiles>1</p:maximumFiles>
            <p:labelCode>p:labelCode</p:labelCode>
