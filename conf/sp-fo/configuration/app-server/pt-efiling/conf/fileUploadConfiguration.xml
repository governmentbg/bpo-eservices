    <p:fileUpload fieldName="inventorsAreRealFiles">
        <p:fileUploadInfo>
            <p:description>false</p:description>
            <p:maximumFiles>1</p:maximumFiles>
            <p:labelCode>p:labelCode</p:labelCode>
            <p:allowedTypes>
                <p:allowedFileType allowedSize="10485760" value="application/pdf" description="PDF"/>
            </p:allowedTypes>
        </p:fileUploadInfo>
    </p:fileUpload>
    <p:fileUpload fieldName="otherAttachments">
        <p:fileUploadInfo>
            <p:description>true</p:description>
            <p:maximumFiles>15</p:maximumFiles>
            <p:labelCode>p:labelCode</p:labelCode>
            <p:allowedTypes>
                <p:allowedFileType allowedSize="10485760" value="application/pdf" description="PDF" />
                <p:allowedFileType allowedSize="10485760" value="application/msword" description="DOC" />
                <p:allowedFileType allowedSize="10485760" value="application/vnd.openxmlformats-officedocument.wordprocessingml.document" description="DOCX" />
                <p:allowedFileType allowedSize="10485760" value="application/vnd.ms-excel" description="XLS" />
                <p:allowedFileType allowedSize="10485760" value="application/vnd.openxmlformats-officedocument.spreadsheetml.sheet" description="XLSX" />
                <p:allowedFileType allowedSize="10485760" value="application/vnd.ms-powerpoint" description="PPT" />
                <p:allowedFileType allowedSize="10485760" value="application/vnd.openxmlformats-officedocument.presentationml.presentation" description="PPTX" />
                <p:allowedFileType allowedSize="10485760" value="audio/mp3" description="MP3"/>
                <p:allowedFileType allowedSize="10485760" value="audio/mpeg"/>
                <p:allowedFileType allowedSize="10485760" value="video/mp4" description="MP4"/>
            </p:allowedTypes>
        </p:fileUploadInfo>
    </p:fileUpload>
    <p:fileUpload fieldName="representativeAttachment">
        <p:fileUploadInfo>
            <p:description>true</p:description>
            <p:maximumFiles>1</p:maximumFiles>
            <p:labelCode>p:labelCode</p:labelCode>
            <p:allowedTypes>
                <p:allowedFileType allowedSize="3145728" value="image/jpeg" description="JPEG" />
                <p:allowedFileType allowedSize="3145728" value="image/pjpeg" />
                <p:allowedFileType allowedSize="3145728" value="image/tiff"  description="TIFF" />
                <p:allowedFileType allowedSize="3145728" value="application/pdf" description="PDF" />
            </p:allowedTypes>
        </p:fileUploadInfo>
    </p:fileUpload>
    <p:fileUpload fieldName="inventorAttachment">
        <p:fileUploadInfo>
            <p:description>true</p:description>
            <p:maximumFiles>15</p:maximumFiles>
            <p:labelCode>p:labelCode</p:labelCode>
            <p:allowedTypes>
                <p:allowedFileType allowedSize="2097152" value="application/pdf" description="PDF" />
                <p:allowedFileType allowedSize="2097152" value="application/msword" description="DOC" />
                <p:allowedFileType allowedSize="2097152" value="application/vnd.openxmlformats-officedocument.wordprocessingml.document" description="DOCX" />
                <p:allowedFileType allowedSize="2097152" value="application/vnd.ms-excel" description="XLS" />
                <p:allowedFileType allowedSize="2097152" value="application/vnd.openxmlformats-officedocument.spreadsheetml.sheet" description="XLSX" />
                <p:allowedFileType allowedSize="2097152" value="application/vnd.ms-powerpoint" description="PPT" />
                <p:allowedFileType allowedSize="2097152" value="application/vnd.openxmlformats-officedocument.presentationml.presentation" description="PPTX" />
            </p:allowedTypes>
        </p:fileUploadInfo>
    </p:fileUpload>
    <p:fileUpload fieldName="loadApplicationXML">
        <p:fileUploadInfo>
            <p:description>true</p:description>
            <p:maximumFiles>1</p:maximumFiles>
            <p:labelCode>p:labelCode</p:labelCode>
            <p:allowedTypes>
                <p:allowedFileType allowedSize="2097152" value="text/xml" description="XML"/>
                <p:allowedFileType allowedSize="2097152" value="application/epub"/>
            </p:allowedTypes>
        </p:fileUploadInfo>
    </p:fileUpload>
</p:fileUploadConfiguration>
