            <p:maximumFiles>1</p:maximumFiles>
            <p:labelCode>p:labelCode</p:labelCode>
            <p:allowedTypes>
                <p:allowedFileType allowedSize="10485760" value="text/xml" description="XML (text/xml)"/>
                <p:allowedFileType allowedSize="10485760" value="application/xml" description="XML (application/xml)"/>
            </p:allowedTypes>
        </p:fileUploadInfo>
    </p:fileUpload>
    <p:fileUpload fieldName="patentOfficiaryFiles">
        <p:fileUploadInfo>
            <p:description>false</p:description>
            <p:maximumFiles>1</p:maximumFiles>
            <p:labelCode>p:labelCode</p:labelCode>
            <p:allowedTypes>
                <p:allowedFileType allowedSize="10485760" value="application/pdf" description="PDF"/>
