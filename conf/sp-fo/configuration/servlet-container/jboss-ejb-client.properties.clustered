endpoint.name=client-endpoint
remote.connectionprovider.create.options.org.xnio.Options.SSL_ENABLED=false

remote.connections=master,slave

remote.connection.master.host=${remote.connection.master.host}
remote.connection.master.port=${remote.connection.master.port}
remote.connection.master.username=${remote.connection.master.username}
remote.connection.master.password=${remote.connection.master.password}
remote.connection.master.connect.options.org.xnio.Options.SASL_POLICY_NOANONYMOUS=false
remote.connection.master.channel.options.org.jboss.remoting3.RemotingOptions.MAX_OUTBOUND_MESSAGES=200

remote.connection.slave.host=${remote.connection.slave.host}
remote.connection.slave.port=${remote.connection.slave.port}
remote.connection.slave.username=${remote.connection.slave.username}
remote.connection.slave.password=${remote.connection.slave.password}
remote.connection.slave.connect.options.org.xnio.Options.SASL_POLICY_NOANONYMOUS=false
remote.connection.slave.channel.options.org.jboss.remoting3.RemotingOptions.MAX_OUTBOUND_MESSAGES=200
