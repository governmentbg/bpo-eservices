<?xml version="1.0" encoding="UTF-8"?>
<smooks-resource-list xmlns="http://www.milyn.org/xsd/smooks-1.1.xsd"
				xmlns:jb="http://www.milyn.org/xsd/smooks/javabean-1.2.xsd" 
				xmlns:core="http://www.milyn.org/xsd/smooks/smooks-core-1.4.xsd">

	<core:filterSettings type="DOM" />
	<jb:bean beanId="CollectionTerms" class="java.util.ArrayList"
		createOnElement="/Transaction/TradeMarkTransactionBody/TransactionContentDetails/TransactionData/GoodsServicesDetails/">
		<jb:wiring beanIdRef="term" />
	</jb:bean>
	<jb:bean beanId="term"
		class="eu.ohim.sp.external.domain.classification.wrapper.Term"
		createOnElement="//GoodsServicesDetails/GoodServices/ClassDescriptionDetails/ClassDescription">
		<jb:value
			data="//GoodsServicesDetails/GoodServices/ClassDescriptionDetails/ClassDescription/ClassNumber"
			decoder="Integer" property="niceClass" />
		<jb:value
			data="//GoodsServicesDetails/GoodServices/ClassDescriptionDetails/ClassDescription/GoodsServicesDescription "
			property="text" />
		<jb:value
			data="//GoodsServicesDetails/GoodServices/ClassDescriptionDetails/ClassDescription/GoodsServicesDescription/@languageCode "
			property="lang" />
	</jb:bean>
</smooks-resource-list>
